<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context=".Fragments.ProcessFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@layout/border"
            android:elevation="5dp"
            android:layout_marginTop="5dp"
            >
            <TextView
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="CLIENTE"
            android:textColor="#686060"
                android:textSize="17sp"
            android:layout_marginBottom="6dp"
            />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000"
                android:layout_marginLeft="10dp"
                />
            <TextView
                android:layout_marginLeft="10dp"
                android:id="@+id/nameProcess"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ssdfsadfasdfdd"
                android:textSize="17sp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="13dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@layout/border"
            android:elevation="5dp"

            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="EXPEDIENTE"
                android:textSize="17sp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"

                />
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000"
                android:layout_marginLeft="10dp"
                />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                >

                <TextView
                    android:id="@+id/expediente"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="12dp"

                    android:text="aaropnm xd"/>

            </LinearLayout>




        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="13dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@layout/border"
            android:elevation="5dp"

            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="JUZGADO"
                android:textSize="17sp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"

                />
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000"
                android:layout_marginLeft="10dp"
                />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                >

                <TextView
                    android:id="@+id/name_juzgado"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="12dp"
                    android:text="el que sea xd xd"
                    />

            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@layout/border"
            android:elevation="5dp"
            android:layout_marginTop="5dp"
            >
            <TextView
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ESPECIALISTA"
            android:textColor="#686060"
            android:textSize="17sp"
            android:layout_marginBottom="6dp"
            />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000"
                android:layout_marginLeft="10dp"
                />
            <TextView
                android:layout_marginLeft="10dp"
                android:id="@+id/name_especialista"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ssdfsadfasdfdd"
                android:textSize="17sp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="13dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@layout/border"
            android:elevation="5dp"

            >
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="DETALLES DEL PROCESO"
                android:textSize="17sp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000"
                android:layout_marginLeft="10dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="10dp">

                <TextView
                    android:layout_marginTop="10dp"
                    android:id="@+id/tipoProceso"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ssdfsadfasdfdd"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CONTRARIO"
                    android:textColor="#686060"
                    android:textSize="12sp"
                    />

                <TextView
                    android:layout_marginTop="10dp"
                    android:id="@+id/jurisdiccion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ssdfsadfasdfdd"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="JUEZ A CARGO"
                    android:textColor="#686060"
                    android:textSize="12sp"
                    />
                <TextView
                    android:layout_marginTop="10dp"
                    android:id="@+id/rJuicio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ssdfsadfasdfdd"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TIPO DE PROCESO "
                    android:textColor="#686060"
                    android:textSize="12sp"
                    />
                <TextView
                    android:layout_marginTop="10dp"
                    android:id="@+id/juezEncargado"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ssdfsadfasdfdd"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ETAPA DEL PROCESO"
                    android:textColor="#686060"
                    android:textSize="12sp"
                    />
                <TextView
                    android:layout_marginTop="10dp"
                    android:id="@+id/etapaCaso"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ssdfsadfasdfdd"
                    android:textSize="17sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="INSTANCIA"
                    android:textColor="#686060"
                    android:textSize="12sp" />
                <TextView
                    android:layout_marginTop="10dp"
                    android:id="@+id/asuento"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ssdfsadfasdfdd"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NOTAS"
                    android:textColor="#686060"
                    android:textSize="12sp"
                    />



            </LinearLayout>

        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="65dp" />
    </LinearLayout>

</ScrollView>

